    <script type="text/javascript">

    $('#where').autocomplete({
      source: function (request, response) {
          $.getJSON("/assets/linkedin_geo_codes.json?term=" + request.term, function (data) {
              response($.map(data.geocodes, function (value, key) {
                  return {
                      codes: value,
                      description: key
                  };
              }));
          });
      },
      minLength: 3,
      delay: 100
  });
    </script>
